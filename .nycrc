{
  "all": true,
  "check-coverage": true,
  "extension": [
    ".ts"
  ],
  "include": [
    "app/**/*.ts"
  ],
  "exclude": [
    "app/locale/**",
    "app/log/**",
    "app/views/**",
    "app/tests/**",
    "app/core/config/**",
    "app/core/logger/**",
    "app/core/types/**",
    "public/**"
  ],
  "excludeNodeModules": true,
  "reporter": [
    "html", "text"
  ],
  "branches": 90,
  "lines": 90,
  "functions": 90,
  "statements": 90
}
